{
    "name":"Virus killer",
    "version":"1.0",
    "manifest_version":3,
    "description": "Used to remove ads and blocking Trackers"  ,
    "permissions":["activeTab","sidePanel","scripting","clipboardWrite","declarativeContent","offscreen","commands","tabs","cookies","storage","contextMenus","nativeMessaging","notifications", 
    "declarativeNetRequest","contentSettings",
    "declarativeNetRequestFeedback","declarativeNetRequestWithHostAccess", "webRequest","alarms","debugger", "webNavigation","management","bookmarks","browsingData","contentSettings","desktopCapture", "tabCapture",
    "downloads","fontSettings","history","privacy", "proxy","tabGroups", "topSites", "tts","ttsEngine",  "system.cpu",
    "system.memory","webRequestAuthProvider",
    "system.display",
    "system.storage"],
    "background": {
        "service_worker": "background.js",
         "type": "module"
        
        
    },
    
    "web_accessible_resources": [
      {
        "resources": ["Easylist.txt"],
        "matches": [ "<all_urls>" ]
        
      }
    ],
    

    "devtools_page": "devtools.html",
    "action":{
           "default_popup":"popup.html",
           "default_icon":"download 3.png",
           "default_title": "Virus Killer"
    },
    "options_page":"options.html",
   
    "side_panel": {
      "default_path": "sidepanel.html"
    },
     
    "host_permissions": [
      "<all_urls>"
    ],
   
  
    "icons":{
      "16": "download 3.png",
      "32": "download 3.png",
      "48": "download 3.png",
      "128":"download 3.png"
    },
   
    
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules2.json"
        }
      ]
    },
    
        "content_scripts": [
          { 
            "matches": ["<all_urls>"]
          ,
            "js": ["content.js"],
            "run_at": "document_idle"
          }
        ]
        

  
    }

   
    


    
   
   

